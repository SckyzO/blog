{
  "extends": ["stylelint-config-standard"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["tailwind", "apply", "variants", "responsive", "screen", "layer", "plugin", "theme", "import", "source", "utility", "custom-variant"]
      }
    ],
    "no-empty-source": null,
    "import-notation": "string"
  }
}
